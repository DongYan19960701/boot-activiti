<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qxf.mapper.LeaveMapper">

    <insert id="addLeave" parameterType="com.qxf.pojo.Leave">
        INSERT INTO `leave`(id,user_id,process_instance_id,start_date,end_date,reason) VALUES(#{id},#{userId},#{processInstanceId},#{startDate},#{endDate},#{reason})
    </insert>

</mapper>